#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/jfrit/source/repos/Codrex.old/DeveloperDWNLD64/node_modules/.pnpm/autoprefixer@10.4.2_postcss@8.4.6/node_modules/autoprefixer/bin/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex.old/DeveloperDWNLD64/node_modules/.pnpm/autoprefixer@10.4.2_postcss@8.4.6/node_modules/autoprefixer/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex.old/DeveloperDWNLD64/node_modules/.pnpm/autoprefixer@10.4.2_postcss@8.4.6/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex.old/DeveloperDWNLD64/node_modules/.pnpm/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex.old/DeveloperDWNLD64/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex.old/node_modules:/mnt/c/Users/jfrit/source/repos/node_modules:/mnt/c/Users/jfrit/source/node_modules:/mnt/c/Users/jfrit/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex.old/DeveloperDWNLD64/node_modules/autoprefixer/bin/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex.old/DeveloperDWNLD64/node_modules/autoprefixer/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/jfrit/source/repos/Codrex.old/DeveloperDWNLD64/node_modules/.pnpm/autoprefixer@10.4.2_postcss@8.4.6/node_modules/autoprefixer/bin/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex.old/DeveloperDWNLD64/node_modules/.pnpm/autoprefixer@10.4.2_postcss@8.4.6/node_modules/autoprefixer/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex.old/DeveloperDWNLD64/node_modules/.pnpm/autoprefixer@10.4.2_postcss@8.4.6/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex.old/DeveloperDWNLD64/node_modules/.pnpm/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex.old/DeveloperDWNLD64/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex.old/node_modules:/mnt/c/Users/jfrit/source/repos/node_modules:/mnt/c/Users/jfrit/source/node_modules:/mnt/c/Users/jfrit/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex.old/DeveloperDWNLD64/node_modules/autoprefixer/bin/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex.old/DeveloperDWNLD64/node_modules/autoprefixer/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../autoprefixer/bin/autoprefixer" "$@"
else
  exec node  "$basedir/../autoprefixer/bin/autoprefixer" "$@"
fi
