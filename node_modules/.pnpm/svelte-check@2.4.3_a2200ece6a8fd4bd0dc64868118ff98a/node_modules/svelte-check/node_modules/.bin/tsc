#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/jfrit/source/repos/Codrex.old/DeveloperDWNLD64/node_modules/.pnpm/typescript@4.5.5/node_modules/typescript/bin/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex.old/DeveloperDWNLD64/node_modules/.pnpm/typescript@4.5.5/node_modules/typescript/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex.old/DeveloperDWNLD64/node_modules/.pnpm/typescript@4.5.5/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex.old/DeveloperDWNLD64/node_modules/.pnpm/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex.old/DeveloperDWNLD64/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex.old/node_modules:/mnt/c/Users/jfrit/source/repos/node_modules:/mnt/c/Users/jfrit/source/node_modules:/mnt/c/Users/jfrit/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex.old/DeveloperDWNLD64/node_modules/.pnpm/svelte-check@2.4.3_a2200ece6a8fd4bd0dc64868118ff98a/node_modules/svelte-check/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex.old/DeveloperDWNLD64/node_modules/.pnpm/svelte-check@2.4.3_a2200ece6a8fd4bd0dc64868118ff98a/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/jfrit/source/repos/Codrex.old/DeveloperDWNLD64/node_modules/.pnpm/typescript@4.5.5/node_modules/typescript/bin/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex.old/DeveloperDWNLD64/node_modules/.pnpm/typescript@4.5.5/node_modules/typescript/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex.old/DeveloperDWNLD64/node_modules/.pnpm/typescript@4.5.5/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex.old/DeveloperDWNLD64/node_modules/.pnpm/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex.old/DeveloperDWNLD64/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex.old/node_modules:/mnt/c/Users/jfrit/source/repos/node_modules:/mnt/c/Users/jfrit/source/node_modules:/mnt/c/Users/jfrit/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex.old/DeveloperDWNLD64/node_modules/.pnpm/svelte-check@2.4.3_a2200ece6a8fd4bd0dc64868118ff98a/node_modules/svelte-check/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex.old/DeveloperDWNLD64/node_modules/.pnpm/svelte-check@2.4.3_a2200ece6a8fd4bd0dc64868118ff98a/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@4.5.5/node_modules/typescript/bin/tsc" "$@"
else
  exec node  "$basedir/../../../../../typescript@4.5.5/node_modules/typescript/bin/tsc" "$@"
