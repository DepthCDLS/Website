#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/jfrit/source/repos/Codrex/DeveloperWEB64/node_modules/.pnpm/esbuild@0.13.15/node_modules/esbuild/bin/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex/DeveloperWEB64/node_modules/.pnpm/esbuild@0.13.15/node_modules/esbuild/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex/DeveloperWEB64/node_modules/.pnpm/esbuild@0.13.15/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex/DeveloperWEB64/node_modules/.pnpm/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex/DeveloperWEB64/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex/node_modules:/mnt/c/Users/jfrit/source/repos/node_modules:/mnt/c/Users/jfrit/source/node_modules:/mnt/c/Users/jfrit/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex/DeveloperWEB64/node_modules/.pnpm/@sveltejs+adapter-vercel@1.0.0-next.40/node_modules/@sveltejs/adapter-vercel/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex/DeveloperWEB64/node_modules/.pnpm/@sveltejs+adapter-vercel@1.0.0-next.40/node_modules/@sveltejs/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex/DeveloperWEB64/node_modules/.pnpm/@sveltejs+adapter-vercel@1.0.0-next.40/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/jfrit/source/repos/Codrex/DeveloperWEB64/node_modules/.pnpm/esbuild@0.13.15/node_modules/esbuild/bin/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex/DeveloperWEB64/node_modules/.pnpm/esbuild@0.13.15/node_modules/esbuild/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex/DeveloperWEB64/node_modules/.pnpm/esbuild@0.13.15/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex/DeveloperWEB64/node_modules/.pnpm/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex/DeveloperWEB64/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex/node_modules:/mnt/c/Users/jfrit/source/repos/node_modules:/mnt/c/Users/jfrit/source/node_modules:/mnt/c/Users/jfrit/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex/DeveloperWEB64/node_modules/.pnpm/@sveltejs+adapter-vercel@1.0.0-next.40/node_modules/@sveltejs/adapter-vercel/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex/DeveloperWEB64/node_modules/.pnpm/@sveltejs+adapter-vercel@1.0.0-next.40/node_modules/@sveltejs/node_modules:/mnt/c/Users/jfrit/source/repos/Codrex/DeveloperWEB64/node_modules/.pnpm/@sveltejs+adapter-vercel@1.0.0-next.40/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../esbuild@0.13.15/node_modules/esbuild/bin/esbuild" "$@"
else
  exec node  "$basedir/../../../../../../esbuild@0.13.15/node_modules/esbuild/bin/esbuild" "$@"
fi
